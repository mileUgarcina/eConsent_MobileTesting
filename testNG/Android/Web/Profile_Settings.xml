<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Set - Profile Settings">

<!--Device Parameter-->
<!--    <parameter name="deviceParameter" value="localAndroidCapsWeb"/>-->
<!--    <parameter name="deviceParameter" value="SauceLabs_Android_RealDevice_Web"/>-->
    <parameter name="deviceParameter" value="BrowserStack_Android_RealDevice_Web"/>
    <parameter name="platformParameter" value="Android"/>
    <parameter name="browserName" value="Chrome"/>
<!--TC Parameter-->
    <parameter name="user" value="Participant"/>
    <parameter name="crete_Xray_Test_Run" value="false"/>

    <test name="Profile Settings">
        <classes>
            <class name="testing.appium.eConsentTesting.Web.user_Menu.Profile_Settings">

                <methods>
                    <include name="participant_App_Mobile_User_Menu_Profile_Settings_Opened"/>
                    <include name="participant_App_Mobile_User_Menu_Profile_Settings_Back_Button"/>
                    <include name="participant_Auth_Mobile_User_Menu_Profile_Settings_Profile_Name_fields_respond_to_valid_data"/>
                    <include name="participant_Auth_Mobile_User_Menu_Profile_Settings_Profile_Name_fields_cleared"/>
                    <include name="participant_Auth_Mobile_User_Menu_Profile_Settings_Profile_updated_with_the_same_input"/>
                    <include name="participant_Auth_Mobile_User_Menu_Profile_Settings_Profile_Name_Changed_Canceled"/>
                    <include name="participant_Auth_Mobile_User_Menu_Profile_Settings_Profile_Name_Changed"/>
                    <include name="participant_Auth_Mobile_User_Menu_Profile_Settings_Language_changed"/>
                </methods>

            </class>
        </classes>
    </test>
</suite>


