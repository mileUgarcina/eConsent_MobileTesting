<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Test Set - Session Activity">

<!--Device Parameter-->
    <parameter name="deviceParameter" value="Local_Android_Emulator_Web"/>
<!--    <parameter name="deviceParameter" value="SauceLabs_Android_RealDevice_Web"/>-->
<!--    <parameter name="deviceParameter" value="BrowserStack_Android_RealDevice_Web"/>-->
    <parameter name="platformParameter" value="Android"/>
    <parameter name="browserName" value="Chrome"/>
<!--TC Parameter-->
    <parameter name="user" value="Participant"/>
    <parameter name="crete_Xray_Test_Run" value="false"/>


    <test name="Session Activity">
        <classes>
            <class name="testing.appium.eConsentTesting.Web.landing_Page.Session_Activity">

                <methods>
                    <include name="participant_App_Mobile_Participant_Session_Expires_13min"/>
                    <include name="participant_App_Mobile_Participant_Session_Expires_15min"/>
                </methods>

            </class>
        </classes>
    </test>
</suite>


