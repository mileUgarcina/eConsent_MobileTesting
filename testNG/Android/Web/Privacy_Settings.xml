<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Privacy Settings">

<!--Device Parameter-->
<!--    <parameter name="deviceParameter" value="localAndroidCapsWeb"/>-->
<!--    <parameter name="deviceParameter" value="SauceLabs_Android_RealDevice_Web"/>-->
    <parameter name="deviceParameter" value="BrowserStack_Android_RealDevice_Web"/>
    <parameter name="platformParameter" value="Android"/>
    <parameter name="browserName" value="Chrome"/>
<!--TC Parameter-->
    <parameter name="user" value="Participant"/>
    <parameter name="crete_Xray_Test_Run" value="false"/>

    <test name="Privacy Settings">
        <classes>
            <class name="testing.appium.eConsentTesting.Web.user_Menu.Privacy_Settings">

                <methods>
                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Opened"/>
                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Privacy_Policy_Link"/>
                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Back_Button_with_no_changes_made"/>
                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Back_Button_with_changes_made"/>
                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Updated_Reload"/>
                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Updated_Save"/>

<!--                    <include name="participant_App_Mobile_User_Menu_Privacy_Settings_Updated_Cancel"/>-->
                </methods>

            </class>
        </classes>
    </test>
</suite>


