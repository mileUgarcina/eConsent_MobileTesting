    <parameter name="emptyPassword" value=""/>

    <test name="Email">
        <classes>
            <class name="testing.appium.eConsentTesting.Web.email.Email">

                <methods>
                    <include name="participant_Auth_Mobile_Sign_In_Account_active_email"/>

                </methods>

            </class>
        </classes>
